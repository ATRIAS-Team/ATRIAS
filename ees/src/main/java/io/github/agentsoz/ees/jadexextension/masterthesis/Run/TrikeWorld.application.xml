<?xml version="1.0" encoding="UTF-8"?><!--
	<H3>The trike world application descriptor</H3>

	It can be started to launch a trike world scenario.
--><applicationtype xmlns="http://www.activecomponents.org/jadex-application" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="TrikeWorld" package="io.github.agentsoz.ees.jadexextension.masterthesis.Run" xsi:schemaLocation="http://www.activecomponents.org/jadex-application                       http://www.activecomponents.org/jadex-application-${jadex_build_version}.xsd">
		<!--name="TrikeWorld" package="jadex.bdiv3.examples.trikeworld">-->
	<componenttypes>

		<componenttype filename="io/github/agentsoz/ees/jadexextension/masterthesis/JadexAgent/TrikeAgent.class" name="TrikeAgent"/>
		<componenttype filename="io/github/agentsoz/ees/jadexextension/masterthesis/JadexAgent/SimIDMapper.class" name="SimIDMapper"/>
		<componenttype filename="io/github/agentsoz/ees/jadexextension/masterthesis/JadexAgent/AreaAgent.class" name="AreaAgent"/>
		<componenttype filename="io/github/agentsoz/ees/jadexextension/masterthesis/JadexAgent/SimSensoryInputBroker.class" name="SimSensoryInputBroker"/>


	</componenttypes>


		<configuration name="One trike local">
			<components>
					<component number="32" type="TrikeAgent"/>
			</components>
			<components>
				<component number="1" type="SimIDMapper"/>
			</components>
			<components>
				<component number="7" type="AreaAgent"/>
			</components>
			<components>
				<component number="2" type="SimSensoryInputBroker"/>
			</components>
		</configuration>


</applicationtype>